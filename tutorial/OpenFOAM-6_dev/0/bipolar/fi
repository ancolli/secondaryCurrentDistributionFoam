FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/bipolar";
    object      fi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 1 2 -3 0 0 -1 0 ];

internalField   uniform 5;

boundaryField
{
        ".*"
    	{
            type            zeroGradient;
    	}

	bipolar_to_electrolyte
        {    
            type	    regionCoupleSolidFluid;
	    side	    solid;
	    nbrField        fi;
	    value           $internalField;
 	    j0		    (9.38e-2 -2.09);
	    b		    (0.0485 -0.0621347);
            E0		    (1.23 0.0);
            kappaMethod     lookup;
            kappa           ks;
            
        }

        frontAndBack
    	{
            type            empty;
    	}
}


