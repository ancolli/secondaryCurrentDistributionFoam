fi
{
    internalField   uniform 10;

    boundaryField
    {
	
        ".*"
    	{
            type            zeroGradient;
    	}

	top
    	{
	    type          fixedValue;
            value	  uniform 10; 
	    //type        fixedGradient;
            //gradient 	uniform xx;// where xx is A/m2 of the current feeder
	}

	anode_to_electrolyte
        {    
            type	    regionCoupleSolid;
	    Tnbr            fi;
	    value           $internalField;
 	    j0		    (9.38e-2);
	    b		    (0.0485);
            E0		    (1.23);
            kappaMethod     lookup;
            kappa           ks;    
        }

        frontAndBack
    	{
            type            empty;
    	}

    }
}

ks
    {
        internalField   uniform 300;  // S/m

        boundaryField
        {    
	    ".*"
            {
                type            zeroGradient;
            }

	    frontAndBack
    	    {
                type            empty;
    	    }

