FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      changeDictionaryDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

fi
{
    internalField   uniform 0;

    boundaryField
    {
	
        ".*"
    	{
            type            zeroGradient;
    	}

	top
    	{
            type            fixedValue;
            value           uniform 0; 
    	}
        
	cathode_to_electrolyte
        {
  	    type	    regionCoupleSolidFluid;
	    side	    solid;
	    nbrField        fi;
	    value           $internalField;
	    j0		    (-2.09);
	    b		    (-0.0621347);
            E0		    (0.0);
            kappaMethod     lookup;
            kappa           ks;    
        }

	frontAndBack
    	{
            type            empty;
    	}

    }
}

ks
    {
        internalField   uniform 300;  // S/m

        boundaryField
        {
	    ".*"
            {
                type            zeroGradient;
            }

	    frontAndBack
    	    {
                type            empty;
    	    }
        }
    }


